{
  "schemaVersion": 1,
  "id": "astraea-spark",
  "provides": [
    "spark"
  ],
  "version": "${version}",
  "name": "Astraea Spark",
  "description": " A Legacy Fabric 1.8.9 adapter for the Spark performance profiler.",
  "authors": [
    "Crosby"
  ],
  "contact": {
    "sources": "https://github.com/AstraeaDevelopment/astraea-spark",
    "issues": "https://github.com/AstraeaDevelopment/astraea-spark/issues"
  },
  "license": "GPL-3.0-or-later",
  "icon": "assets/astraea-spark/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.astraea.spark.LegacyFabricSparkMod"
    ],
    "client": [
      "io.github.astraea.spark.LegacyFabricSparkMod::initializeClient"
    ]
  },
  "mixins": [
    "astraea-spark.mixins.json"
  ],
  "accessWidener": "astraea-spark.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.19",
    "minecraft": "1.8.9"
  }
